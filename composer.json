{
    "name": "sylius/admin-order-creation-plugin",
    "type": "sylius-plugin",
    "description": "Sylius Plugin for order creation in Admin panel",
    "license": "MIT",
    "authors": [
        {
            "name": "Mateusz Zalewski",
            "homepage": "http://mpzalewski.com.pl"
        },
        {
            "name": "Grzegorz Sadowski",
            "email": "sadowskigp@gmail.com"
        },
        {
            "name": "Sylius Team",
            "email": "team@sylius.com"
        }
    ],
    "require": {
        "php": "^7.2",

        "sylius/sylius": "~1.3.0",
        "friendsofsymfony/jsrouting-bundle": "^2.2"
    },
    "require-dev": {
        "behat/behat": "^3.3",
        "behat/mink": "^1.7@dev",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-selenium2-driver": "^1.3",
        "friends-of-behat/context-service-extension": "^1.2",
        "friends-of-behat/cross-container-extension": "^1.1",
        "friends-of-behat/page-object-extension": "^0.2.0",
        "friends-of-behat/service-container-extension": "^1.0",
        "friends-of-behat/symfony-extension": "^1.2.1",
        "friends-of-behat/variadic-extension": "^1.1",
        "lakion/mink-debug-extension": "^1.2.3",
        "phpspec/phpspec": "^4.0",
        "phpstan/phpstan-shim": "^0.9.2",
        "phpunit/phpunit": "^6.5",
        "se/selenium-server-standalone": "^2.52",
        "sylius-labs/coding-standard": "^2.0",
        "symfony/web-server-bundle": "^3.4|^4.1",
        "symfony/debug-bundle": "^3.4|^4.1",
        "symfony/web-profiler-bundle": "^3.4|^4.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Sylius\\AdminOrderCreationPlugin\\": "src/",
            "Tests\\Sylius\\AdminOrderCreationPlugin\\": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/Application/app/AppKernel.php", "tests/Application/app/TestAppKernel.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
